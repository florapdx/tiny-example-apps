{
  "name": "sockets-test-server",
  "version": "1.0.0",
  "description": "A test server for running socket load and other perf tests",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "build:ui": "babel ui --out-dir build",
    "build:src": "babel src --out-dir build",
    "build:schema": "generate-graphql src/schema.js src/schema.graphql",
    "build": "npm run build:ui && npm run build:src",
    "watch:ui": "webpack -w --config webpack/config.js",
    "watch:src": "babel src --watch --out-dir build",
    "watch:relay": "relay-compiler --src ui --schema src/schema.graphql --watch",
    "watch": "concurrently \"npm run watch:relay\" \"npm run watch:ui\" \"npm run watch:src\"",
    "flow": "flow",
    "test": "npm test"
  },
  "keywords": [
    "websocket",
    "testing",
    "socket",
    "testing",
    "websockets",
    "tests"
  ],
  "author": "picks&shovels",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-relay": "^1.4.1",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "ccxt": "^1.9.168",
    "concurrently": "^3.5.0",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "farce": "^0.2.3",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.57.3",
    "found": "^0.3.5",
    "found-relay": "^0.3.0-alpha.9",
    "graphql": "^0.11.7",
    "graphql-relay": "^0.5.3",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "iterall": "^1.1.3",
    "postcss-loader": "^2.0.8",
    "promise-polyfill": "6.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-relay": "^1.4.1",
    "redis": "^2.8.0",
    "relay-compiler": "^1.4.1",
    "relay-runtime": "^1.4.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "uws": "^8.14.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "benchtable": "^0.1.0",
    "fast-json-stringify": "^0.14.0",
    "generate-graphql": "^1.0.1",
    "microtime": "^2.1.6",
    "msgpack-lite": "^0.1.26",
    "notepack": "0.0.2"
  }
}
